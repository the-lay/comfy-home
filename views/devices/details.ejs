<div class="col-sm-10">

  <div class="row">
    <div class="col-xs-12">
      
      <div class="row">
        <div class="col-md-12">

          <h2 class="sub-header">Nodes in the system</h2>
          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
                <tr>
                  <th>ID</th>
                  <th>Name</th>
                  <th>Node type</th>
                  <th>Unit</th>
                  <th>Update interval</th>
                  <th>Created at</th>
                  <th>Updated at</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td><%=node.id%></td>
                  <td><%=node.name%></td>
                  <td><%=node.nodeConfiguration.nodeType%></td>
                  <td><%=node.nodeConfiguration.unit%></td>
                  <td><%=node.nodeConfiguration.updateInterval%></td>
                  <td><%=node.createdAt%></td>
                  <td><%=node.updatedAt%></td>
                </tr>
              </tbody>
            </table>
          </div>


        </div>
      </div>
    </div>
  </div>

</div>